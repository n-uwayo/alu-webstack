defaults
        mode    http
        option  httplog
        option  dontlognull
        timeout connect 5000
        timeout client  50000
        timeout server  50000

frontend http-in
        bind *:80
        bind *:443 ssl crt /etc/letsencrypt/live/www.noella.tech/fullchain.pem
        mode http
        default_backend webservers

backend webservers
        balance roundrobin
        server web-01 50.19.145.215:80 check
        server web-02 52.55.102.40:80 check


frontend https_frontend
        bind *:80
        bind *:443 ssl crt /etc/letsencrypt/live/www.noella.tech/fullchain.pem
        mode http
        default_backend webservers
